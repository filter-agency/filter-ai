{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "title": "FAQs",
  "description": "A block to generate FAQs",
  "keywords": ["FAQ", "FAQs", "Frequently Asked Questions"],
  "version": "1.0.0",
  "textdomain": "filter-ai",
  "name": "filter-ai/faqs",
  "icon": "editor-ul",
  "category": "filter-ai",
  "attributes": {
    "title": {
      "type": "string"
    },
    "title_align": {
      "type": "string",
      "default": "center"
    },
    "background_color": {
      "type": "string",
      "default": "#fff"
    },
    "heading_color": {
      "type": "string",
      "default": "#000"
    }
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "supports": {
    "html": false,
    "anchor": true,
    "alignWide": true
  },
  "providesContext": {
    "filter-ai/faqs/heading_color": "heading_color"
  }
}
